package biz.digitalhouse.integration.v3.services.bingoGame.dto;

/**
 * @author Vitalii Babenko
 *         on 04.03.2016.
 */
public class BingoWinnerDTO {

    private int partNumber;
    private String winners;
    private String pattern;
    private int winningCallNumber;

    public BingoWinnerDTO(int partNumber, String winners, String pattern, int winningCallNumber) {
        this.partNumber = partNumber;
        this.winners = winners;
        this.pattern = pattern;
        this.winningCallNumber = winningCallNumber;
    }

    public int getPartNumber() {
        return partNumber;
    }

    public String getWinners() {
        return winners;
    }

    public String getPattern() {
        return pattern;
    }

    public int getWinningCallNumber() {
        return winningCallNumber;
    }

    @Override
    public String toString() {
        return "BingoWinnerDTO{" +
                "partNumber=" + partNumber +
                ", winners='" + winners + '\'' +
                ", pattern='" + pattern + '\'' +
                ", winningCallNumber=" + winningCallNumber +
                '}';
    }
}
