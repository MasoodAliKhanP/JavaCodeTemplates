public class ActiveRoom {

    private long roomID;
    private String roomName;
    private double cardCost;
    private double gamePrize;
    private long nextGameStart;
    private Double jackpotAmount;
    private int playersBoughtCards;
    private int playersInRoom;

    public ActiveRoom(long roomID, String roomName, double cardCost, double gamePrize, long nextGameStart, Double jackpotAmount, int playersBoughtCards, int playersInRoom) {
        this.roomID = roomID;
        this.roomName = roomName;
        this.cardCost = cardCost;
        this.gamePrize = gamePrize;
        this.nextGameStart = nextGameStart;
        this.jackpotAmount = jackpotAmount;
        this.playersBoughtCards = playersBoughtCards;
        this.playersInRoom = playersInRoom;
    }

    public long getRoomID() {
        return roomID;
    }

    public String getRoomName() {
        return roomName;
    }

    public double getCardCost() {
        return cardCost;
    }

    public double getGamePrize() {
        return gamePrize;
    }

    public long getNextGameStart() {
        return nextGameStart;
    }

    public Double getJackpotAmount() {
        return jackpotAmount;
    }

    public int getPlayersBoughtCards() {
        return playersBoughtCards;
    }

    public int getPlayersInRoom() {
        return playersInRoom;
    }

    @Override
    public String toString() {
        return "ActiveRoom{" +
                "roomID=" + roomID +
                ", roomName='" + roomName + '\'' +
                ", cardCost=" + cardCost +
                ", gamePrize=" + gamePrize +
                ", nextGameStart=" + nextGameStart +
                ", jackpotAmount=" + jackpotAmount +
                ", playersBoughtCards=" + playersBoughtCards +
                ", playersInRoom=" + playersInRoom +
                '}';
    }
}
