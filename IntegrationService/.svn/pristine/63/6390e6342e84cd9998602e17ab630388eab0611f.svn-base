package biz.digitalhouse.integration.v3.web.ws.http.bingoGameAPI.dto;

import java.util.Date;

/**
 * Created by arbuzov
 * on 20.03.2018.
 */
public class PlayerHistoryRound {

    private String roundID;
    private String roomName;
    private Date roundDate;
    private Double price;
    private int cardsNumber;
    private Double prize;

    public PlayerHistoryRound() {
    }

    public PlayerHistoryRound(String roundID, String roomName, Date roundDate, Double price, int cardsNumber, Double prize) {
        this.roundID = roundID;
        this.roomName = roomName;
        this.roundDate = roundDate;
        this.price = price;
        this.cardsNumber = cardsNumber;
        this.prize = prize;
    }

    public String getRoundID() {
        return roundID;
    }

    public void setRoundID(String roundID) {
        this.roundID = roundID;
    }

    public String getRoomName() {
        return roomName;
    }

    public void setRoomName(String roomName) {
        this.roomName = roomName;
    }

    public Date getRoundDate() {
        return roundDate;
    }

    public void setRoundDate(Date roundDate) {
        this.roundDate = roundDate;
    }

    public Double getPrice() {
        return price;
    }

    public void setPrice(Double price) {
        this.price = price;
    }

    public int getCardsNumber() {
        return cardsNumber;
    }

    public void setCardsNumber(int cardsNumber) {
        this.cardsNumber = cardsNumber;
    }

    public Double getPrize() {
        return prize;
    }

    public void setPrize(Double prize) {
        this.prize = prize;
    }

    @Override
    public String toString() {
        return "PlayerHistoryRound{" +
                "roundID='" + roundID + '\'' +
                ", roomName='" + roomName + '\'' +
                ", roundDate=" + roundDate +
                ", price=" + price +
                ", cardsNumber=" + cardsNumber +
                ", prize=" + prize +
                '}';
    }
}
