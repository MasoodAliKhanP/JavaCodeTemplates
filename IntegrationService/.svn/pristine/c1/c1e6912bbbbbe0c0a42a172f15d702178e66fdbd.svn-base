package biz.digitalhouse.integration.v3.services.externalClient.oldHttpResponse;

/**
 * <p>Class: BetResultResponse</p>
 * <p>Description: </p>
 *
 * @author Sergey Miliaev
 */

public class BetResultResponse extends BaseTransactionalResponse {

    private String currency;
    private Double cash;
    private Double bonus;

    public String getCurrency() {
        return currency;
    }

    public void setCurrency(String currency) {
        this.currency = currency;
    }

    public Double getCash() {
        return cash;
    }

    public void setCash(Double cash) {
        this.cash = cash;
    }

    public Double getBonus() {
        return bonus;
    }

    public void setBonus(Double bonus) {
        this.bonus = bonus;
    }

    @Override
    public String toString() {
        return "BetResultResponse{" +
                "currency='" + currency + '\'' +
                ", cash=" + cash +
                ", bonus=" + bonus +
                "} " + super.toString();
    }
}
