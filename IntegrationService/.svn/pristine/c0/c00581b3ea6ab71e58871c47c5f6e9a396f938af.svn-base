package biz.digitalhouse.integration.v3.services.externalClient.dto;

/**
 * @author Vitalii Babenko
 *         on 10.04.2016.
 */
public class RefundResult {

    private int status;
    private String extTransactionId;

    public RefundResult(int status) {
        this.status = status;
    }

    public RefundResult(int status, String extTransactionId) {
        this.status = status;
        this.extTransactionId = extTransactionId;
    }

    public int getStatus() {
        return status;
    }

    public String getExtTransactionId() {
        return extTransactionId;
    }

    @Override
    public String toString() {
        return "RefundResult{" +
                "status=" + status +
                ", extTransactionId='" + extTransactionId + '\'' +
                '}';
    }
}
