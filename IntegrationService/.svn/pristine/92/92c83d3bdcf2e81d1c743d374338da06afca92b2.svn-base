package biz.digitalhouse.integration.v3.services.bingoGame.dto;

/**
 * @author Vitalii Babenko
 *         on 01.03.2016.
 */
public class PreorderCardDTO extends BaseResponseDTO {

    private long gameID;
    private int serialNumber;
    private int cardNumber;

    public PreorderCardDTO(long gameID, int serialNumber, int cardNumber) {
        this.gameID = gameID;
        this.serialNumber = serialNumber;
        this.cardNumber = cardNumber;
    }

    public long getGameID() {
        return gameID;
    }

    public int getSerialNumber() {
        return serialNumber;
    }

    public int getCardNumber() {
        return cardNumber;
    }

    public void setCardNumber(int cardNumber) {
        this.cardNumber = cardNumber;
    }

    @Override
    public String toString() {
        return "PreorderCardDTO{" +
                "gameID=" + gameID +
                ", serialNumber=" + serialNumber +
                ", cardNumber=" + cardNumber +
                '}';
    }
}
