package biz.digitalhouse.integration.v3.services.freeRoundsBonus.dto;

import java.util.Date;

/**
 * Created by Vitalii Babenko
 * on 13.01.2016.
 */
public class InfoFRBonus {

    private String freeRoundBonusID;
    private Date createDate;
    private Date expireDate;
    private String games;
    private int initialRounds;
    private int playedRounds;

    public String getFreeRoundBonusID() {
        return freeRoundBonusID;
    }

    public void setFreeRoundBonusID(String freeRoundBonusID) {
        this.freeRoundBonusID = freeRoundBonusID;
    }

    public Date getCreateDate() {
        return createDate;
    }

    public void setCreateDate(Date createDate) {
        this.createDate = createDate;
    }

    public Date getExpireDate() {
        return expireDate;
    }

    public void setExpireDate(Date expireDate) {
        this.expireDate = expireDate;
    }

    public String getGames() {
        return games;
    }

    public void setGames(String games) {
        this.games = games;
    }

    public int getInitialRounds() {
        return initialRounds;
    }

    public void setInitialRounds(int initialRounds) {
        this.initialRounds = initialRounds;
    }

    public int getPlayedRounds() {
        return playedRounds;
    }

    public void setPlayedRounds(int playedRounds) {
        this.playedRounds = playedRounds;
    }

    @Override
    public String toString() {
        return "InfoFRBonus{" +
                "freeRoundBonusID='" + freeRoundBonusID + '\'' +
                ", createDate=" + createDate +
                ", expireDate=" + expireDate +
                ", games='" + games + '\'' +
                ", initialRounds=" + initialRounds +
                ", playedRounds=" + playedRounds +
                '}';
    }
}
